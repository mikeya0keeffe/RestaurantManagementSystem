<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="244.0" prefWidth="307.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginwindows.Logger">
   <children>
      <TextField fx:id="UserName" layoutX="70.0" layoutY="60.0" prefHeight="30.0" prefWidth="160.0" promptText="UserName" />
      <Label fx:id="loginStatus" layoutX="70.0" layoutY="30.0" prefHeight="17.0" prefWidth="160.0" text="Login" />
      <PasswordField fx:id="Password" layoutX="70.0" layoutY="110.0" minHeight="30.0" prefWidth="160.0" promptText="Password" />
      <Button fx:id="LoginButton" layoutX="70.0" layoutY="155.0" mnemonicParsing="false" onAction="#loginAttempt" prefHeight="30.0" prefWidth="160.0" text="Login" />
      <Button layoutX="70.0" layoutY="195.0" mnemonicParsing="false" onAction="#setUpNewRestaurant" prefHeight="30.0" prefWidth="160.0" text="New restaurant" />
   </children>
</AnchorPane>
